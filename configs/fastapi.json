{
  "name": "fastapi",
  "description": "FastAPI basics, path operations, query parameters, request body handling",
  "base_url": "https://fastapi.tiangolo.com/tutorial/",
  "selectors": {
    "main_content": "article",
    "title": "h1",
    "code_blocks": "pre code"
  },
  "url_patterns": {
    "include": [
      "/tutorial/"
    ],
    "exclude": [
      "/img/",
      "/js/",
      "/css/"
    ]
  },
  "rate_limit": 0.5,
  "max_pages": 500,
  "_router": true,
  "_sub_skills": [
    "fastapi-basics",
    "fastapi-advanced"
  ],
  "_routing_keywords": {
    "fastapi-basics": [
      "getting_started",
      "request_body",
      "validation",
      "basics"
    ],
    "fastapi-advanced": [
      "async",
      "dependencies",
      "security",
      "advanced"
    ]
  }
}